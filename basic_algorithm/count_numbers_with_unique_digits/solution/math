class Solution:
    def countNumbersWithUniqueDigits(self, n):
        """
        :type n: int
        :rtype: int
        """
        if n < 0:
            return 0
        if 0 == n:
            return 1
        
        cnt, product = 10, 9
        
        for i in range(2, n + 1):
            product *= 11 - i
            cnt += product
            
        return cnt
        
