class Solution:
    def countNumbersWithUniqueDigits(self, n):
        """
        :type n: int
        :rtype: int
        """
        if 0 == n:
            return 1
        if 0 > n:
            return 0
        
        cnt = 10
        for i in range(2, n + 1):
            product = 9
            for j in range(2, i + 1):
                product *= 11 - j
            cnt += product
            
        return cnt
        
